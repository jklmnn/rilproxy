service rilproxy_client /system/bin/logwrapper /system/bin/rilproxy_client /dev/socket/rild $interface
    class late_start
    user root
    group root
    seclabel u:r:adbd:s0

service rilnet /system/bin/rilproxy_client.sh $interface
        class late_start
        user root
        group root
        seclabel u:r:adbd:s0
        disabled
        oneshot

on property:dev.bootcomplete=1
        stop zygote
        start rilnet
